<?php

$input = intval(readline());
for ($i = 2; $i <= $input; $i++) {
    $checker = true;
    for ($j = 2; $j < $i; $j++) {
        if ($i % $j == 0) {
            $checker = false;
            break;
        }
    }

    if ($checker)
        printf("%d -> true" . PHP_EOL, $i);
    else
        printf("%d -> false" . PHP_EOL, $i);
}
