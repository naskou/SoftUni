<?php

    $count = intval(readline());
    $arr = [];
    $passengersCount = 0;

    for ($i = 0; $i < $count; $i++) {

        $passengers = intval(readline());
        $arr[$i] = $passengers;
        $passengersCount += $passengers;
    }
    echo implode(' ', $arr) . PHP_EOL;
    echo $passengersCount;
