function toggle() {
        
        let button = $('.button');
        let $extraDiv = $('#extra');
        
        if ($extraDiv.css('display') === 'none') {
            $extraDiv.css('display', 'block');
            button.text('Less');
        } else {
            $extraDiv.css('display', 'none');
            button.text('More')
        }
    }
