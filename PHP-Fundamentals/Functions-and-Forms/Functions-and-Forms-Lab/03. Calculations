<?php

    $command = readline();
    $a = readline();
    $b = readline();
    $result = 0;

    calculate($command, $a, $b, $result);

    function calculate($command, $a, $b, $result)
    {
        switch ($command) {
            case 'add':
                $result = $a + $b;
                break;
            case 'multiply':
                $result = $a * $b;
                break;
            case 'subtract':
                $result = $a - $b;
                break;
            case 'divide':
                $result = $a / $b;
                break;
        }
        echo $result;
    }
